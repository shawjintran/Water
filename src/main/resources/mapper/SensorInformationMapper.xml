<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.t.water.mapper.SensorInformationMapper">

    <resultMap id="BaseResultMap" type="com.t.water.entity.SensorInformation">
            <id property="sensorId" column="sensor_id" jdbcType="BIGINT"/>
            <result property="sensorLastTurbidity" column="sensor_last_turbidity" jdbcType="VARCHAR"/>
            <result property="sensorLastCod" column="sensor_last_cod" jdbcType="VARCHAR"/>
            <result property="sensorLastTds" column="sensor_last_tds" jdbcType="VARCHAR"/>
            <result property="sensorLastPh" column="sensor_last_ph" jdbcType="VARCHAR"/>
            <result property="sensorLastTemperature" column="sensor_last_temperature" jdbcType="DECIMAL"/>
            <result property="sensorPumpStatus" column="sensor_pump_status" jdbcType="TINYINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        sensor_id,sensor_last_turbidity,sensor_last_cod,
        sensor_last_tds,sensor_last_ph,sensor_last_temperature,
        sensor_pump_status,create_time,update_time,
        is_deleted
    </sql>
</mapper>
